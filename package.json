{
  "name": "StoreControl",
  "productName": "StoreControl",
  "version": "0.3.0",
  "description": "StoreControl",
  "repository": "https://github.com/dougmaitelli/StoreControl",
  "license": "UNLICENSED",
  "main": "main.js",
  "scripts": {
    "install-wsl": "npm install && npm uninstall electron && export npm_config_platform=win32 && npm install electron && unset npm_config_platform",
    "start": "electron .",
    "dev": "electron . --debug",
    "lint": "standard",
    "pack-win": "electron-packager . StoreControl --overwrite --platform=win32 --arch=ia32 --out=release",
    "pack-mac": "electron-packager . StoreControl --overwrite --platform=darwin --arch=x64 --out=release",
    "pack-linux": "electron-packager . StoreControl --overwrite --platform=linux --arch=x64 --out=release",
    "package": "npm run pack-win && npm run pack-mac && npm run pack-linux && node ./script/installer.js"
  },
  "keywords": [],
  "author": "Douglas Maitelli",
  "homepage": "",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.7",
    "angular": "^1.6.6",
    "animate.css": "^3.5.2",
    "chart.js": "^2.6.0",
    "electron": "^1.7.9",
    "electron-gh-releases": "^2.0.4",
    "format-as-currency": "^3.1.3",
    "jquery": "^3.2.1",
    "jquery-mask-plugin": "^1.14.12",
    "nedb": "^1.8.0",
    "semantic-ui-css": "^2.2.12",
    "sweetalert": "^1.1.3"
  },
  "devDependencies": {
    "electron-packager": "^8.7.1",
    "electron-winstaller": "^2.6.3"
  }
}
